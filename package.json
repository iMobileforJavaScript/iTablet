{
  "name": "iTablet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-rn": "node node_modules/react-native/local-cli/cli.js start",
    "start": "react-native run-ios",
    "start:android": "react-native run-android",
    "start:reset": "react-native run-ios --no-packager & npm run start-rn -- --reset-cache",
    "start:android:reset": "react-native run-android --no-packager && npm run start-rn -- --reset-cache",
    "installRelease:android": "cd android && ./gradlew installRelease",
    "installDebug:android": "cd android && ./gradlew installDebug",
    "assembleRelease": "cd android && ./gradlew assembleRelease",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "precommit": "lint-staged",
    "prepush": "",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.js": [
      "prettier --single-quote --trailing-comma all --no-semi --write",
      "eslint src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "forge": "^2.3.0",
    "immutable": "^3.8.2",
    "jcore-react-native": "^1.3.2",
    "jest-serializer": "^23.0.1",
    "jpush-react-native": "^2.5.3",
    "node-forge": "^0.7.6",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "0.57.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-amap-geolocation": "^1.0.2",
    "react-native-camera": "^2.9.0",
    "react-native-camera-roll-picker": "^1.2.3",
    "react-native-color-picker": "^0.4.0",
    "react-native-contacts": "^4.0.1",
    "react-native-fs": "^2.13.3",
    "react-native-gifted-chat": "^0.7.2",
    "react-native-image-crop-picker": "^0.24.0",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-nav": "^2.0.2",
    "react-native-orientation": "^3.1.3",
    "react-native-progress": "^3.5.0",
    "react-native-root-toast": "^3.0.1",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-search-box": "^0.0.16",
    "react-native-splash-screen": "^3.1.1",
    "react-native-swipeout": "^2.3.6",
    "react-native-swiper": "^1.5.13",
    "react-native-table-component": "^1.1.8",
    "react-native-video": "^4.4.1",
    "react-native-xml2js": "^1.0.3",
    "react-navigation": "^1.5.11",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-persist": "^5.10.0",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "teaset": "^0.6.3"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.1",
    "lint-staged": "^7.3.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier": "^1.14.3",
    "react-devtools-core": "3.3.4",
    "react-test-renderer": "16.5.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
